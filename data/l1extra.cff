# For use with 1_2_0_pre2

#
# Calo emulator
#

include "L1Trigger/GlobalCaloTrigger/data/L1CaloEmuDigis.cff"

replace hcalTriggerPrimitiveDigis.weights = {1, 1}
replace hcalTriggerPrimitiveDigis.peakFilter = true

es_source = HcalTPGCoderULUT {
	FileInPath filename='CalibCalorimetry/HcalTPGAlgos/data/inputLUTcoder.dat'
}
es_source = CaloTPGTranscoderULUTs {
	FileInPath hcalLUT='CalibCalorimetry/CaloTPG/data/outputLUTtranscoder.dat'
}

#
# Muon emulator
#

include "Geometry/CSCGeometry/data/cscGeometry.cfi"
include "Geometry/DTGeometry/data/dtGeometry.cfi"
include "Geometry/RPCGeometry/data/rpcGeometry.cfi" 

include "L1Trigger/GlobalMuonTrigger/data/l1muon.cff"

#
# L1Extra
#

include "L1Trigger/L1ExtraFromDigis/data/l1extraParticles.cfi"
include "L1Trigger/L1ExtraFromDigis/data/l1extraParticleMap.cfi"

#
# Sequence definitions
#

sequence l1emulator = { L1CaloEmuDigis, l1muon }
sequence l1extra = { l1extraParticles, l1extraParticleMap }
